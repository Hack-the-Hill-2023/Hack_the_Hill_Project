{
    "manifest_version": 3,
    "name": "Canadian Tire Bundle Extension",
    "description": "Code injection bundle",
    "version": "1.0",

    "permissions": [
        "activeTab"
    ],

    "action": {
        "default_icon": "img/logo.png"
    },
  
    "content_scripts": [
        {
            "matches": ["*://www.canadiantire.ca/en.html"],
            "js": ["content-script.js"]
        }
    ]
}